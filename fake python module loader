@echo off
title PyCharm Game Modules Installer
echo Downloading PyCharm modules for games (to play on desktop)...
echo Please wait, this may take a moment...

:: Run pip installs quietly
start /b cmd /c "python -m pip install --upgrade pip >nul 2>&1"
start /b cmd /c "python -m pip install pygame chess ursina >nul 2>&1"

:: Delay for install tasks
timeout /t 15 >nul

echo Installation complete! You can now play games with Python.

